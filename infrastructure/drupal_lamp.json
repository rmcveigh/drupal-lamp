{
  "nfs_exports": {
    "/srv/www/peeksi/current":  {
      "clients": {
        "*": {
          "writeable": true,
          "sync": false,
          "options": [
            "no_subtree_check",
            "all_squash",
            "async"
          ],
          "anon_user_name": "www-data",
          "anon_group_name": "www-data"
        }
      }
    }
  },
  "drupal": {
    "sites": {
      "peeksi": {
        "active": true,
        "deploy": {
          "action": ["deploy", "install"],
          "releases": 1
        },
        "drush_make": {
          "api": "2",
          "files": {
            "default": "build-peeksi.make",
            "core": "drupal-org-core.make"
          },
          "template": true
        },
        "drupal": {
          "version": "7.26",
          "install": {
            "install_configure_form.update_status_module": "'array(FALSE,FALSE)'",
            "--clean-url": 1
          },
          "settings": {
            "profile": "standard",
            "files": "sites/default/files",
            "cookbook": "drupal",
            "settings": {
              "default": {
                "location": "sites/default/settings.php"
              }
            },
            "db_name": "peeksi",
            "db_host": "localhost",
            "db_prefix": "",
            "db_driver": "mysql",
            "db_file": ""
          }
        },
        "repository": {
          "host": "github.com",
          "uri": "git@github.com:b-ry/peeksi.git",
          "revision": "master"
        },
        "web_app": {
          "80": {
            "server_name": "peeksi.local",
            "rewrite_engine": "On",
            "docroot": "/srv/www/peeksi/current"
          }
        }
      }
    }
  }
}
